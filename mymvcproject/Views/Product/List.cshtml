
@{
    ViewData["Title"] = "List";
}

<h1> Product List</h1>

<table class="table table-bordered">
    <tr>
        <th>PRODUCT ID</th>
        <th>PRODUCT NAME</th>
        <th>PRICE</th>
    </tr>
    @foreach(Product p in (IEnumerable<Product>) ViewData["productlist"])
    {
        <tr>
            <td>@p.Id</td> 
            <td>@p.Name</td>
            <td>@p.Price</td>
        </tr>
        
    }
</table>

<h4>using Viewbag</h4>
<table class="table table-bordered">
    <tr>
        <th>PRODUCT ID</th>
        <th>PRODUCT NAME</th>
        <th>PRICE</th>
    </tr>
    @foreach(Product p in ViewBag.Products)
    {
        <tr>
            <td>@p.Id</td> 
            <td>@p.Name</td>
            <td>@p.Price</td>
        </tr>
        
    }
</table>